# OpenAI API Key - Used for AI/ML functionality and natural language processing
OPENAI_API_KEY=your_openai_api_key

# MongoDB Connection URIs
# Test environment database
MONGODB_TEST_URI=your_test_mongodb_uri
# Production environment database 
MONGODB_PROD_URI=your_prod_mongodb_uri

# Privy Authentication Configuration
# Used for secure user authentication and wallet management
# Development environment Privy credentials
PRIVY_DEV_APP_ID=your_dev_app_id
PRIVY_DEV_APP_SECRET=your_dev_app_secret
PRIVY_DEV_WALLET_AUTHORIZATION_KEY=your_dev_wallet_auth_key

# Production environment Privy credentials
PRIVY_PROD_APP_ID=your_prod_app_id
PRIVY_PROD_APP_SECRET=your_prod_app_secret 
PRIVY_PROD_WALLET_AUTHORIZATION_KEY=your_prod_wallet_auth_key

# Development network endpoint for testing
SONIC_TESTNET_RPC_URL=https://rpc.ankr.com/sonic_blaze_testnet
# Production network endpoint
SONIC_MAINNET_RPC_URL=https://rpc.ankr.com/sonic_mainnet

# Sonic API Key
SONIC_API_KEY=your_sonic_api_key

# Frontend Application URL - Required for CORS and callback configurations
FRONTEND_URL=http://localhost:3000

# Dune API Key - Used for accessing Dune API
DUNE_API_KEY=your_dune_api_key

# Server Port
PORT=5000
