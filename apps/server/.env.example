PRIVY_DEV_WALLET_AUTHORIZATION_KEY=your_dev_wallet_auth_key

# Production environment Privy credentials
PRIVY_PROD_APP_ID=your_prod_app_id
PRIVY_PROD_APP_SECRET=your_prod_app_secret 
PRIVY_PROD_WALLET_AUTHORIZATION_KEY=your_prod_wallet_auth_key

