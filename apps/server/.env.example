# OpenAI API Key - Used for AI/ML functionality and natural language processing
OPENAI_API_KEY=your_openai_api_key

# Anthropic API Key - Used for Claude AI model
ANTHROPIC_API_KEY=your_anthropic_api_key

# MongoDB Connection URIs
# Test environment database
MONGODB_TEST_URI=your_test_mongodb_uri
# Production environment database 
MONGODB_PROD_URI=your_prod_mongodb_uri

# Privy Authentication Configuration
# Used for secure user authentication and wallet management
# Development environment Privy credentials
PRIVY_DEV_APP_ID=your_dev_app_id
